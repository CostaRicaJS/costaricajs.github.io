<div class="alignleft">
    <div class="header-container">
        <div class="header-container-section">
            <img class="header-logo-image" src="/img/CostaRicaJS-Sloth.png" />
        </div>
        <div class="header-container-section">
            <h1><a href="<%- config.root %>"><%= config.title %></a></h1>
            <h2><a href="<%- config.root %>"><% if (config.subtitle){ %><%= config.subtitle %><% } %></a></h2>
        </div>
    </div>
</div>
<nav id="main-nav">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
  <div class="clearfix"></div>
</nav>
<div class="clearfix"></div>